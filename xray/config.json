{
  "log": {
    "loglevel": "warning"
  },
  "inbounds": [{
    "port": 10000,
    "protocol": "vless",
    "settings": {
      "clients": [{
        "id": "your-uuid",
        "flow": "xtls-rprx-direct"
      }]
    },
    "streamSettings": {
      "network": "tcp",
      "security": "xtls",
      "xtlsSettings": {
        "alpn": ["http/1.1"],
        "certificates": [{
          "certificateFile": "/etc/ssl/private/xray.crt",
          "keyFile": "/etc/ssl/private/xray.key"
        }]
      }
    }
  }],
  "outbounds": [{
    "protocol": "freedom",
    "settings": {}
  }]
}